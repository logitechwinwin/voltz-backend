    "nestjs-form-data": "^1.9.91",
    "nestjs-typeorm-paginate": "^4.0.4",
    "pg": "^8.12.0",
    "pg-query-stream": "^4.6.0",
    "pretty-format": "^29.7.0",
    "reflect-metadata": "^0.2.0",
    "request-ip": "^3.3.0",
    "res": "^0.4.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "ua-parser-js": "^1.0.38",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.2",
    "xmlbuilder2": "^3.1.1"
